# Environment Variables - Example File
# Copy this file to .env and fill in your actual API keys

# Google Gemini API key for AI assistant
# For Supabase Edge Functions (used in /supabase/functions/ai-assistant)
GEMINI_API_KEY=your_gemini_api_key_here

# For client-side fallback in browser (Used in AIAssistant.tsx)
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration (for client-side)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Supabase Function URL for AI Assistant (Used in AIAssistant.tsx) - Production URL
VITE_SUPABASE_FUNCTION_URL=https://your-project-ref.supabase.co/functions/v1/ai-assistant

# Resend API key for email functionality
RESEND_API_KEY=your_resend_api_key_here
